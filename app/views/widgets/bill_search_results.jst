<% if (bill_search_results.length > 0) { %>
<ul id="searchResults">
  <% _.each(bill_search_results, function(billContainer) {
      var bill = billContainer.bill; %>
    <li><a class="billSearchResult" href="#" data-bill="<%= bill.ident %>"><strong><%= bill.typenumber %></strong> &mdash; <%= bill.best_title(:plain).substring(0, 120) + (bill.best_title(:plain).length > 120 ? "..." : '') %></a></li>
  <% }); %>
</div>
<% } else { %>
Sorry, no matching bills were found.
<% } %>
