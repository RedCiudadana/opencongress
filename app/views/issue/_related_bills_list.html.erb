<div id="related_bills_list">

<table>
<tr><th>Bill</th><th>Status</th><th>Last Action</th></tr>
<% related_bills_list.each do |bill| -%>
<tr><td><%= link_to bill.typenumber, bill_path(bill) %> <%= bill.any_title(:bare) %></td><td><%=bill.status %></td><td><%= bill.last_action.formatted_date %></td></tr>
<% end -%>
</table>

</div>
